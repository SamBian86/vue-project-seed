{
  "name": "yunlin-cloud-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && build.prod.sh",
    "build:sit": "vue-cli-service build --mode production.sit",
    "build:test": "vue-cli-service build --mode production.test",
    "lint": "vue-cli-service lint",
    "et": "node_modules/.bin/et",
    "et:init": "node_modules/.bin/et -i",
    "et:list": "gulp themes"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "babel-eslint": "^8.0.1",
    "babel-plugin-component": "^1.1.1",
    "core-js": "^3.6.4",
    "echarts": "^4.8.0",
    "element-theme": "^2.0.1",
    "element-ui": "^2.13.1",
    "js-cookie": "^2.2.1",
    "screenfull": "^5.0.2",
    "vue": "^2.6.11",
    "vue-i18n": "^8.17.1",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3",
    "wangeditor": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "element-theme-chalk": "^2.13.1",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-load-plugins": "^2.0.3",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.0.2",
    "sass-loader": "^8.0.2",
    "svg-sprite-loader": "^4.2.5",
    "vue-template-compiler": "^2.6.11"
  },
  "element-theme": {
    "config": "./src/element-ui/theme-variables.scss",
    "out": "./src/element-ui/theme",
    "minimize": true,
    "browsers": [
      "> 1%",
      "last 2 versions",
      "not ie <= 10"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
